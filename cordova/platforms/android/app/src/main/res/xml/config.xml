<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="109002" id="com.sydneytrainwalks.ios" version="1.9.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Train Walks</name>
    <description>
		Easy nature walks around Sydney using the train, bus and ferry.
	</description>
    <author email="maurice@sydneytrainwalks.com" href="http://www.sydneytrainwalks.com">
		Maurice van Creij
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="assets/logos/icon-36.png" />
    <icon density="mdpi" src="assets/logos/icon-48.png" />
    <icon density="hdpi" src="assets/logos/icon-72.png" />
    <icon density="xhdpi" src="assets/logos/icon-96.png" />
    <icon density="xxhdpi" src="assets/logos/icon-144.png" />
    <icon density="xxxhdpi" src="assets/logos/icon-192.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="StatusBarOverlaysWebView" value="true" />
</widget>
