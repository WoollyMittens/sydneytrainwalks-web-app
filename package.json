{
  "name": "sydneytrainwalks",
  "version": "0.0.0",
  "devDependencies": {
    "canvas": "",
    "del": "",
    "exif": "",
    "fs": "",
    "gm": "",
    "imagemin-pngquant": "",
    "jsdom": "",
    "request": "",
    "sass": "",
    "shelljs": "",
    "uglify-js": "",
    "xmldom": "",
    "npm-watch": ""
  },
  "watch": {
    "scripts_dev": {
      "patterns": ["src/lib", "src/js"],
      "extensions": "js",
      "quiet": false
    },
    "styles_dev": {
      "patterns": ["src/scss"],
      "extensions": "scss",
      "quiet": false
    }
  },
  "scripts": {
    "importgpx": "cd node_scripts && node importgpx && cd ..",
    "importphotos": "cd node_scripts && node importphotos && cd ..",
    "importexif": "cd node_scripts && node importexif && cd ..",
    "importguides": "cd node_scripts && node importguides && cd ..",
    "importtiles": "cd node_scripts && node importtiles && cd ..",
    "converttiles": "cd node_scripts && node converttiles && cd ..",
    "scripts_dev": "uglifyjs ./src/lib/*.js ./src/js/*.js --output ./inc/js/scripts.js --source-map",
    "scripts_prod": "uglifyjs ./src/lib/*.js ./src/js/*.js --output ./inc/js/scripts.js --compress --mangle --keep-fnames --source-map",
    "styles_dev": "sass ./src/scss/styles.scss ./inc/css/styles.css --style expanded --update",
    "styles_prod": "sass ./src/scss/styles.scss ./inc/css/styles.css --style compressed",
    "styles_watch": "sass --watch ./src/scss:./inc/css --style expanded",
    "assets": "npm run importgpx && npm run importphotos && npm run importexif && npm run importguides && npm run importtiles && npm run converttiles",
    "dev": "scripts_dev && styles_dev",
    "prod": "scripts_prod && styles_prod",
    "build": "npm run assets && npm run scripts_prod && npm run styles_prod",
    "watch": "npm-watch"
  }
}
